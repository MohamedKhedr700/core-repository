{
    "name": "raid/core-repository",
    "description": "Raid Core Repository",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "deoliveiralucas/array-keys-case-transform": "^1.1",
        "raid/core-action": "dev-main",
        "raid/core-auth": "dev-main",
        "raid/core-command": "dev-main",
        "raid/core-event": "dev-main",
        "raid/core-gate": "dev-main",
        "raid/core-model": "dev-main",
        "laravel/pint": "^1.10",
        "spatie/laravel-fractal": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Raid\\Core\\Repository\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raid\\Core\\Repository\\Providers\\RepositoryServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint --config pint.json "
            ]
        },
        "authors": [
        {
            "name": "mohamedkhedr700",
            "email": "mohamedkhedr700@gmail.com"
        }
    ]
}
